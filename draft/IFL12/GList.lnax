%include lhs2TeX.fmt
%include includelhs2tex.lhs

\begin{code}
data GL :  (i -> i -> *) ->
           (i -> i -> *) ->
           (i -> i -> *) where
  GNil   : GL x r i i
  GCons  : x i j -> r j k -> GL x r i k
     deriving fixpoint GList

append l =
  mcata { i j . GList x j k -> GList f i k } l
    with
      app GNil           ys  = ys
      app  (GCons x xs)  ys  =
           gCons x (app xs ys)
\end{code}
